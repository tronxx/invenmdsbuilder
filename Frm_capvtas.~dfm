  object qry_renentra: TQuery
    CachedUpdates = True
    AfterScroll = qry_renentraAfterScroll
    DatabaseName = 'manvehi'
    Filtered = True
    SQL.Strings = (
      'select   IDRENENTRA, IDENTRADA, CONSE, siono, entcan,'
      'a.IDART, a.IDSERIE, FOLENT, FOLSAL, COSTOU, a.piva, prvta,'
      
        'codigo, descri, serie, status, idvend, tipago, persenvrec, idciu' +
        'dad, idrecemi'
      'from renentra a join inven b on a.idart = b.idart'
      'join series c on a.idserie = c.idserie'
      'where a.identrada = :IDENTRADA order by conse')
