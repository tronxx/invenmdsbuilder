  object uqry_entradas: TUpdateSQL
    ModifySQL.Strings = (
      'update entradas'
      'set'
      '  IDRECEMI = :IDRECEMI,'
      '  IDPROVE = :IDPROVE,'
      '  PERENVREC = :PERENVREC,'
      '  FACPRO = :FACPRO,'
      '  STATUS = :STATUS,'
      '  CONIVA = :CONIVA,'
      '  FECHA = :FECHA,'
      '  IMPORTE = :IMPORTE,'
      '  IVA = :IVA,'
      '  TOTAL = :TOTAL,'
      '  VENCE = :VENCE,'
      '  CTOFIN = :CTOFIN,'
      '  TASCOMP = :TASCOMP,'
      '  TASPRO = :TASPRO,'
      '  FECHAFAC = :FECHAFAC,'
      '  LETRAS = :LETRAS,'
      '  PLAZOCFP = :PLAZOCFP,'
      '  IDPLANP = :IDPLANP,'
      '  FLETES = :FLETES,'
      '  DESXAP = :DESXAP,'
      '  FECHAPRP = :FECHAPRP,'
