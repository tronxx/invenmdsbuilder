  object qry_agrepuntos: TQuery
    DatabaseName = 'manvehi'
    SQL.Strings = (
      'insert into infcvtatmp(almkdx) select almac from movart'
      
        'where (modent= :SON1 or modent= :SON2) and (entcan= :CAN1 or ent' +
        'can= :CAN2)'
      'and almac between :PRALM and :ULTALM and fecha <= :FECCORT'
      
        'and (salio<>'#39'S'#39' or (salio='#39'S'#39' and fechasal > :FECCORT)) and cia=' +
        ' :CIA'
      'group by almac')
    Left = 655
    Top = 95
    ParamData = <
      item
        DataType = ftString
        Name = 'SON1'
        ParamType = ptUnknown
      end
      item
        DataType = ftString
        Name = 'SON2'
        ParamType = ptUnknown
      end
      item
        DataType = ftString
        Name = 'CAN1'
        ParamType = ptUnknown
      end
      item
        DataType = ftString
        Name = 'CAN2'
        ParamType = ptUnknown
      end
      item
        DataType = ftString
        Name = 'PRALM'
        ParamType = ptUnknown
      end
      item
        DataType = ftString
        Name = 'ULTALM'
        ParamType = ptUnknown
      end
      item
        DataType = ftDateTime
        Name = 'FECCORT'
        ParamType = ptUnknown
      end
      item
        DataType = ftDateTime
        Name = 'FECCORT'
        ParamType = ptUnknown
      end
      item
        DataType = ftInteger
        Name = 'Cia'
        ParamType = ptUnknown
      end>
  end
  object qry_puntos: TQuery
