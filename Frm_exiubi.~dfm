  object qry_invulcos: TQuery
    DatabaseName = 'manvehi'
    SQL.Strings = (
      
        'select a.costos as si, a.coston as no from invulcos a where a.co' +
        'digo = :CODIGO and a.cia = :CIA and a.fecha = ( select max(fecha' +
        ') from invulcos b where b.codigo = a.codigo and '
      'b.cia = a.cia and b.fecha <= :FECORTE )')
    Left = 664
    Top = 192
    ParamData = <
      item
        DataType = ftString
        Name = 'CODIGO'
        ParamType = ptUnknown
      end
      item
        DataType = ftInteger
        Name = 'CIA'
        ParamType = ptUnknown
      end
      item
        DataType = ftDateTime
        Name = 'FECORTE'
        ParamType = ptUnknown
      end>
  end
