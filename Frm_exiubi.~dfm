    SQL.Strings = (
      'select zona,almkdx from infcvtatmp group by zona,almkdx')
    Left = 624
    Top = 128
  end
  object qry_lineas: TQuery
    DatabaseName = 'manvehi'
    SQL.Strings = (
      'select b.orden, b.numero, b.descri from inven a '
      'join lineas b on a.linea = b.numero '
      
        'where a.prove between :PROVEINI and :PROVEFIN and b.numero betwe' +
        'en :LINEINI and :LINEFIN and a.cia= :CIA'
      'group by orden, numero, b.descri')
    Left = 624
    Top = 160
    ParamData = <
      item
        DataType = ftString
        Name = 'PROVEINI'
        ParamType = ptUnknown
      end
      item
        DataType = ftString
        Name = 'PROVEFIN'
        ParamType = ptUnknown
      end
      item
        DataType = ftString
        Name = 'LINEINI'
        ParamType = ptUnknown
      end
      item
        DataType = ftString
        Name = 'LINEFIN'
        ParamType = ptUnknown
      end
      item
        DataType = ftInteger
        Name = 'CIA'
        ParamType = ptUnknown
      end>
  end
  object qry_inven: TQuery
    DatabaseName = 'manvehi'
    SQL.Strings = (
      
        'select * from inven  where linea  =  :LINEA  and empaqe between ' +
        ':SITUAINI and :SITUAFIN  and prove between :PROVEINI and :PROVEF' +
        'IN  and cia= :CIA'
      'order by codigo')
    Left = 656
    Top = 160
    ParamData = <
      item
        DataType = ftString
        Name = 'LINEA'
        ParamType = ptUnknown
      end
      item
        DataType = ftString
        Name = 'SITUAINI'
        ParamType = ptUnknown
      end
      item
        DataType = ftString
        Name = 'SITUAFIN'
        ParamType = ptUnknown
      end
      item
        DataType = ftString
        Name = 'PROVEINI'
        ParamType = ptUnknown
      end
      item
        DataType = ftString
        Name = 'PROVEFIN'
        ParamType = ptUnknown
      end
      item
        DataType = ftInteger
        Name = 'CIA'
        ParamType = ptUnknown
      end>
  end
  object qry_armar: TQuery
    DatabaseName = 'manvehi'
    SQL.Strings = (
      
        'select count(*) as unids from movart where modent= :MODEN and co' +
        'digo= :COD and'
      
        #9'(salio<>'#39'S'#39' or (salio='#39'S'#39' and fechasal > :FECHA )) and  almac =' +
        ' :ALM'
      #9'  and fecha <= :FECHA and entcan= :ENTCAN and cia= :CIA')
