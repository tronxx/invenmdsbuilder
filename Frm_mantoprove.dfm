  object qry_prove: TQuery
    CachedUpdates = True
    DatabaseName = 'manvehi'
    SQL.Strings = (
      'select codigo, nombre, direc, ciu, rfc, tel, cargos, abonos,'
      'compraanu, comprames, limite, contacto, ultmov, status, cia'
      'from proveedor'
      'where cia = :CIA'
      'order by codigo')
