        Name = 'CIA'
        ParamType = ptUnknown
      end>
  end
  object uqry_clien: TUpdateSQL
    ModifySQL.Strings = (
      'update proveedor'
      'set'
      '  CODIGO = :CODIGO,'
      '  NOMBRE = :NOMBRE,'
      '  DIREC = :DIREC,'
      '  CIU = :CIU,'
      '  RFC = :RFC,'
      '  TEL = :TEL,'
      '  CARGOS = :CARGOS,'
      '  ABONOS = :ABONOS,'
      '  COMPRAANU = :COMPRAANU,'
      '  COMPRAMES = :COMPRAMES,'
      '  LIMITE = :LIMITE,'
      '  CONTACTO = :CONTACTO,'
      '  ULTMOV = :ULTMOV,'
      '  STATUS = :STATUS,'
      '  CIA = :CIA'
      'where'
      '  CODIGO = :OLD_CODIGO and'
      '  CIA = :OLD_CIA')
    InsertSQL.Strings = (
      'insert into proveedor'
      
        '  (CODIGO, NOMBRE, DIREC, CIU, RFC, TEL, CARGOS, ABONOS, COMPRAA' +
        'NU, '
      'COMPRAMES, '
      '   LIMITE, CONTACTO, ULTMOV, STATUS, CIA)'
      'values'
      
        '  (:CODIGO, :NOMBRE, :DIREC, :CIU, :RFC, :TEL, :CARGOS, :ABONOS,' +
        ' '
      ':COMPRAANU, '
      '   :COMPRAMES, :LIMITE, :CONTACTO, :ULTMOV, :STATUS, :CIA)')
    DeleteSQL.Strings = (
      'delete from proveedor'
      'where'
      '  CODIGO = :OLD_CODIGO and'
      '  CIA = :OLD_CIA')
    Left = 600
    Top = 101
  end
