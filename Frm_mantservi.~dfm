  object qry_servicio: TQuery
    CachedUpdates = True
    DatabaseName = 'manvehi'
    SQL.Strings = (
      'select a.idart, a.codigo, a.impservicio, a.tipo, a.fecha, a.cia,'
      'b.linea, b.descri, d.descri as descrilin'
      'from inv_servicios a join inven b on a.codigo = b.codigo'
      'and a.cia = b.cia'
      'join inv_invhist c on a.idart = c.idart and a.cia = c.cia'
      'join inv_lineas d on b.linea = d.linea and a.cia = d.cia'
      'where a.cia = :CIA and a.tipo = :TIPO'
      'order by b.linea, a.codigo'
      ' ')
    UpdateObject = uqry_servicio
    Left = 312
    Top = 32
    ParamData = <
      item
        DataType = ftInteger
        Name = 'CIA'
        ParamType = ptUnknown
      end
      item
        DataType = ftInteger
        Name = 'TIPO'
        ParamType = ptUnknown
      end>
  end
  object uqry_servicio: TUpdateSQL
    ModifySQL.Strings = (
      'update inv_servicios'
      'set'
      '  IMPSERVICIO = :IMPSERVICIO,'
      '  FECHA = :FECHA'
      'where'
      '  IDART = :OLD_IDART and'
      '  TIPO = :OLD_TIPO')
    InsertSQL.Strings = (
