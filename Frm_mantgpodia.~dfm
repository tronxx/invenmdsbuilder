  object qry_gpodiary: TQuery
    CachedUpdates = True
    AfterScroll = qry_gpodiaryAfterScroll
    DatabaseName = 'manvehi'
    SQL.Strings = (
      'select  idgpodiary, grupo, descri, imprime, cia from gpodiary'
      'where cia = :CIA order by grupo')
    UpdateObject = uqry_gpodiary
    Left = 320
    Top = 96
    ParamData = <
      item
        DataType = ftInteger
        Name = 'CIA'
        ParamType = ptUnknown
      end>
  end
  object uqry_gpodiary: TUpdateSQL
    ModifySQL.Strings = (
      'update gpodiary'
      'set'
      '  DESCRI = :DESCRI,'
      '  IMPRIME = :IMPRIME'
      'where'
      '  IDGPODIARY = :OLD_IDGPODIARY')
    InsertSQL.Strings = (
      'insert into gpodiary'
      '  (IDGPODIARY, GRUPO, DESCRI, IMPRIME, CIA)'
      'values'
      '  (:IDGPODIARY, :GRUPO, :DESCRI, :IMPRIME, :CIA)')
    DeleteSQL.Strings = (
      'delete from gpodiary'
      'where'
      '  IDGPODIARY = :OLD_IDGPODIARY')
    Left = 350
    Top = 96
  end
