        DataType = ftInteger
        Name = 'Numero'
        ParamType = ptUnknown
      end
      item
        DataType = ftInteger
        Name = 'Cia'
        ParamType = ptUnknown
      end>
  end
  object qry_renentra: TQuery
    CachedUpdates = True
    AfterScroll = qry_renentraAfterScroll
    DatabaseName = 'manvehi'
    SQL.Strings = (
      
        'select a.tipo,a.alm,a.recemi,a.numero,a.conse,a.codinv,a.serie,a' +
        '.siono,'
      '(case entosal when '#39'E'#39' then folent else NULL end) as folent,'
      '(case entosal when '#39'S'#39' then folsal else NULL end) as folsal,'
      'a.unids, a.costou, a.piva, a.importe, a.cantmueve,'
      'a. status, a.persenvrec, a.cia, a.vend, a.poblac, a.tipago,'
      'a.prvta, a.entosal, a.entcan,'
      'b.descri, c.concepto'
      'from renentra a'
      'join inven b on b.codigo = a.codinv and b.cia = a.Cia'
      'join conceps c on a.persenvrec = c.ncon'
      'where  a.tipo=:Tipo and a.alm=:Alm and a.numero=:Numero'
