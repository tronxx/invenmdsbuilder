  object qsEntradas: TQuery
    CachedUpdates = True
    DatabaseName = 'manvehi'
    SQL.Strings = (
      
        'select TIPO, ALM, RECEMI, NUMERO, FACPRO, PROVE, PERENVREC, STAT' +
        'US,'
      'CONIVA, FECHA, IMPORTE, IVA, TOTAL,'
      
        'cast ( (case  vence when '#39'0000-00-00'#39' then fecha else vence end)' +
        ' as Date ) as vence,'
      'ctofin,tascomp, taspro,'
      
        'cast ( ( case  fechafac when '#39'0000-00-00'#39' then fecha else fechaf' +
        'ac end) as Date ) as fechafac,'
      
        'LETRAS, PLAZOCFP, PLANP, FLETES, DESXAP, FECHAPRP, CTOFINCOMP, U' +
        'SUARIO, CIA'
      ''
      
        'from Entradas where tipo= :Tipo and alm=:Alm and numero= :Numero' +
        ' and cia=:Cia'
      ' ')
    UpdateObject = uqsEntradas
    Left = 216
    Top = 368
    ParamData = <
      item
        DataType = ftString
        Name = 'tipo'
        ParamType = ptUnknown
      end
