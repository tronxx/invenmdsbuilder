  object qry_renentra: TQuery
    CachedUpdates = True
    DatabaseName = 'manvehi'
    Filtered = True
    SQL.Strings = (
      'select h.iddatreltransf, h.idreltransf, h.idrelvtaart, h.cia,'
      
        'g.zonaptvta, a.idrelvtas, a.fecha, a.fechaent, a.entrega, b.sale' +
        'de, a.ptovta,'
      'a.folio, c.concepto as codigo, d.concepto as descri,'
      'e.nombre as poblac, b.preciolista,'
      'f.codigo as tipoentrega'
      'from inv_datreltransf h'
      'join inv_relvtaart b on h.idrelvtaart = b.idrelvtaart'
      'join inv_relvtas a on b.idrelvtas = a.idrelvtas'
      'join conceps c on b.idcodigo  = c.ncon'
      'join conceps d on b.iddescri  = d.ncon'
      'join poblacs e on a.idpoblac = e.numero'
      'join inv_tipoentrega f on a.idtipoentrega = f.idtipoentrega'
      'join ubivtainv g on a.ptovta = g.pvta and g.alm = '#39#39
      'where h.idreltransf = :IDRELTRANSF'
      'order by b.salede, g.zonaptvta, a.ptovta, codigo, descri'
      ' '
      ' ')
    UpdateObject = uqry_renentra
    Left = 375
    Top = 289
    ParamData = <
      item
        DataType = ftInteger
        Name = 'IDRELTRANSF'
        ParamType = ptUnknown
      end>
