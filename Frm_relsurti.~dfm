  object qry_relvtas: TQuery
    CachedUpdates = True
    DatabaseName = 'manvehi'
    SQL.Strings = (
      
        'select g.zonaptvta, a.idrelvtas, a.fecha, a.fechaent, a.entrega,' +
        ' b.salede,'
      'a.ptovta, a.folio, c.concepto as codigo, d.concepto as descri,'
      'e.nombre as poblac, b.preciolista, b.idrelvtaart,'
      'f.codigo as tipoentrega'
      'from inv_relvtas a join inv_relvtaart b'
      'on a.idrelvtas = b.idrelvtas'
      'join conceps c on b.idcodigo  = c.ncon'
      'join conceps d on b.iddescri  = d.ncon'
      'join poblacs e on a.idpoblac = e.numero'
      'join inv_tipoentrega f on a.idtipoentrega = f.idtipoentrega'
      'join ubivtainv g on a.ptovta = g.pvta and g.alm = '#39#39
      'where a.fecha between :FECINI and :FECFIN and a.cia = :CIA'
      'and b.idreltransf = 0'
      'order by g.zonaptvta, b.salede, a.ptovta, codigo, descri'
      ' '
      ' '
      ' ')
    UpdateObject = uqry_relvtas
    Left = 97
    Top = 291
    ParamData = <
      item
        DataType = ftDateTime
        Name = 'FECINI'
        ParamType = ptUnknown
      end
      item
        DataType = ftDateTime
        Name = 'FECFIN'
        ParamType = ptUnknown
      end
      item
        DataType = ftInteger
        Name = 'CIA'
        ParamType = ptUnknown
      end>
