    ModifySQL.Strings = (
      'update inv_reltransf'
      'set'
      '  FECHA = :FECHA,'
      '  FOLIO = :FOLIO,'
      '  IDUSUARIO = :IDUSUARIO,'
      '  HORA = :HORA,'
      '  FECHAINI = :FECHAINI,'
      '  FECHAFIN = :FECHAFIN,'
      '  HORAINI = :HORAINI,'
      '  HORAFIN = :HORAFIN,'
      '  FECCAP = :FECCAP'
      'where'
      '  IDRELTRANSF = :OLD_IDRELTRANSF')
    InsertSQL.Strings = (
      'insert into inv_reltransf'
      
        '  (IDRELTRANSF, FECHA, FOLIO, IDUSUARIO, HORA, FECHAINI, FECHAFI' +
        'N, '
      'HORAINI, '
      '   HORAFIN, FECCAP, CIA)'
      'values'
      
        '  (:IDRELTRANSF, :FECHA, :FOLIO, :IDUSUARIO, :HORA, :FECHAINI, :' +
        'FECHAFIN, '
      '   :HORAINI, :HORAFIN, :FECCAP, :CIA)')
    DeleteSQL.Strings = (
      'delete from inv_reltransf'
      'where'
