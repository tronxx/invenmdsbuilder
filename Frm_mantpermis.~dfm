  object uqry_proyectos: TUpdateSQL
    ModifySQL.Strings = (
      'update fac_proyectos'
      'set'
      '  IDPROYECTO = :IDPROYECTO,'
      '  DESCRI = :DESCRI'
      'where'
      '  IDPROYECTO = :OLD_IDPROYECTO')
    InsertSQL.Strings = (
      'insert into fac_proyectos'
      '  (IDPROYECTO, DESCRI)'
      'values'
      '  (:IDPROYECTO, :DESCRI)')
    DeleteSQL.Strings = (
      'delete from fac_proyectos'
      'where'
      '  IDPROYECTO = :OLD_IDPROYECTO')
    Left = 195
    Top = 160
  end
  object qry_proyectos: TQuery
    CachedUpdates = True
    AfterScroll = qry_proyectosAfterScroll
    DatabaseName = 'manvehi'
    SQL.Strings = (
      'select idproyecto, descri from fac_proyectos')
    UpdateObject = uqry_proyectos
    Left = 195
    Top = 128
  end
  object dts_proyectos: TDataSource
    DataSet = qry_proyectos
    Left = 194
    Top = 192
  end
  object qry_permisiones: TQuery
    CachedUpdates = True
    DatabaseName = 'manvehi'
    SQL.Strings = (
      'select a.idpermis, a.descripcion, a.clave from'
      'car_permisiones a'
      'order by idpermis'
      ' '
      ' ')
    UpdateObject = uqry_permisiones
    Left = 264
    Top = 128
  end
  object uqry_permisiones: TUpdateSQL
    ModifySQL.Strings = (
      'update car_permisiones'
      'set'
      '  DESCRIPCION = :DESCRIPCION, CLAVE = :CLAVE'
      'where'
      '  IDPERMIS = :OLD_IDPERMIS')
    InsertSQL.Strings = (
      'insert into car_permisiones'
      '  (IDPERMIS, DESCRIPCION, clave)'
      'values'
      '  (:IDPERMIS, :DESCRIPCION, :CLAVE)')
    DeleteSQL.Strings = (
      'delete from car_permisiones'
      'where'
      '  IDPERMIS = :OLD_IDPERMIS')
    Left = 264
    Top = 160
  end
  object dts_permisiones: TDataSource
    AutoEdit = False
    DataSet = qry_permisiones
    Left = 263
    Top = 192
  end
  object qry_facpermiproyectos: TQuery
    CachedUpdates = True
    DatabaseName = 'manvehi'
    SQL.Strings = (
      
        'select a.idpermiproyecto, a.idproyecto, a.idpermis, b.descripcio' +
        'n from'
      
        'fac_permiproyectos a join car_permisiones b on a.idpermis = b.id' +
        'permis'
      'where a.idproyecto = :IDPROYECTO'
      'order by descripcion'
      ' ')
    UpdateObject = uqry_facpermiproyectos
    Left = 296
    Top = 128
    ParamData = <
      item
        DataType = ftInteger
        Name = 'IDPROYECTO'
        ParamType = ptUnknown
      end>
  end
  object uqry_facpermiproyectos: TUpdateSQL
    ModifySQL.Strings = (
      'update fac_permiproyectos'
      'set'
      '  IDPERMIPROYECTO = :IDPERMIPROYECTO,'
      '  IDPROYECTO = :IDPROYECTO,'
      '  IDPERMIS = :IDPERMIS'
      'where'
      '  IDPERMIPROYECTO = :OLD_IDPERMIPROYECTO')
    InsertSQL.Strings = (
      'insert into fac_permiproyectos'
      '  (IDPERMIPROYECTO, IDPROYECTO, IDPERMIS)'
      'values'
      '  (:IDPERMIPROYECTO, :IDPROYECTO, :IDPERMIS)')
    DeleteSQL.Strings = (
      'delete from fac_permiproyectos'
      'where'
      '  IDPERMIPROYECTO = :OLD_IDPERMIPROYECTO')
    Left = 296
    Top = 160
  end
  object dts_facpermiproyectos: TDataSource
    DataSet = qry_facpermiproyectos
    Left = 295
    Top = 192
  end
