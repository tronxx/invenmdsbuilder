      '  IDUSUARIO = :IDUSUARIO,'
      '  FECCAP = :FECCAP,'
      '  IDPOLRELVTA = :IDPOLRELVTA,'
      '  TIPOPAGO = :TIPOPAGO'
      'where'
      '  IDRELVTAS = :OLD_IDRELVTAS')
    InsertSQL.Strings = (
      'insert into inv_relvtas'
      
        '  (IDRELVTAS, FOLIO, FECHA, HORA, IDNOMBRE, TIPOCTE, IDTIPOENTRE' +
        'GA, '
      'PTOVTA, '
      
        '   FECHAENT, ENTREGA, VENDEDOR, IDPOBLAC, IDUSUARIO, FECCAP, CIA' +
        ', '
      'IDPOLRELVTA, '
      '   TIPOPAGO)'
      'values'
      '  (:IDRELVTAS, :FOLIO, :FECHA, :HORA, :IDNOMBRE, :TIPOCTE, '
      ':IDTIPOENTREGA, '
      
        '   :PTOVTA, :FECHAENT, :ENTREGA, :VENDEDOR, :IDPOBLAC, :IDUSUARI' +
        'O, '
      ':FECCAP, '
      '   :CIA, :IDPOLRELVTA, :TIPOPAGO)')
    DeleteSQL.Strings = (
