  object qry_agenda: TQuery
    CachedUpdates = True
    AfterScroll = qry_agendaAfterScroll
    DatabaseName = 'manvehi'
    SQL.Strings = (
      
        'select idagenda,folio,fecha,a.ubica,horaini,horafin,idusuario,a.' +
        'cia, b.descri as nombrealm'
      'from inv_agendaentrega a join inv_ubiptovta b'
      'on a.ubica = b.ubica and a.cia = b.cia'
      'where '
      'fecha between :FECHAINI and :FECHAFIN and a.cia = :CIA '
      'order by fecha desc, folio'
      ' ')
    UpdateObject = uqry_agenda
    Left = 264
    Top = 176
    ParamData = <
      item
        DataType = ftDateTime
        Name = 'FECHAINI'
        ParamType = ptUnknown
      end
      item
        DataType = ftDateTime
        Name = 'FECHAFIN'
        ParamType = ptUnknown
      end
      item
        DataType = ftInteger
        Name = 'CIA'
        ParamType = ptUnknown
      end>
  end
