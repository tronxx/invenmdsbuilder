  object qry_relacs: TQuery
    CachedUpdates = True
    DatabaseName = 'manvehi'
    SQL.Strings = (
      
        'select a.idreltabla,a.idtabla,a.campo,a.idtablarel,b.tabla,a.cam' +
        'porel,a.tiporel,a.descri,a.observs'
      
        'from info_car_reltabla a join info_car_tablas b on b.idtabla = a' +
        '.idtablarel '
      'where a.idtabla = :IDTABLA'
      'order by idreltabla ')
    UpdateObject = uqry_relacs
    Left = 365
    Top = 399
    ParamData = <
      item
        DataType = ftInteger
