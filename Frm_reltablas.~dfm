  object uqry_relacs: TUpdateSQL
    ModifySQL.Strings = (
      'update info_car_reltabla'
      'set'
      '  IDRELTABLA = :IDRELTABLA,'
      '  IDTABLA = :IDTABLA,'
      '  CAMPO = :CAMPO,'
      '  IDTABLAREL = :IDTABLAREL,'
      '  CAMPOREL = :CAMPOREL,'
      '  TIPOREL = :TIPOREL,'
      '  DESCRI = :DESCRI,'
      '  OBSERVS = :OBSERVS'
      'where'
      '  IDRELTABLA = :OLD_IDRELTABLA')
    InsertSQL.Strings = (
      'insert into info_car_reltabla'
      
        '  (IDRELTABLA, IDTABLA, CAMPO, IDTABLAREL, CAMPOREL, TIPOREL, DE' +
        'SCRI, '
      'OBSERVS)'
      'values'
      
        '  (:IDRELTABLA, :IDTABLA, :CAMPO, :IDTABLAREL, :CAMPOREL, :TIPOR' +
        'EL, '
      ':DESCRI, '
      '   :OBSERVS)')
    DeleteSQL.Strings = (
      'delete from info_car_reltabla'
      'where'
      '  IDRELTABLA = :OLD_IDRELTABLA')
    Left = 400
    Top = 400
  end
