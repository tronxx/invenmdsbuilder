    ModifySQL.Strings = (
      'update info_car_tablas'
      'set'
      '  IDTABLA = :IDTABLA,'
      '  TABLA = :TABLA,'
      '  DESCRI = :DESCRI,'
      '  FECHA_CREA = :FECHA_CREA'
      'where'
      '  IDTABLA = :OLD_IDTABLA')
    InsertSQL.Strings = (
      'insert into info_car_tablas'
      '  (IDTABLA, TABLA, DESCRI, FECHA_CREA)'
      'values'
      '  (:IDTABLA, :TABLA, :DESCRI, :FECHA_CREA)')
    DeleteSQL.Strings = (
      'delete from info_car_tablas'
      'where'
      '  IDTABLA = :OLD_IDTABLA')
