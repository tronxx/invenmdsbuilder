  object qry_seriesanuanter: TQuery
    DatabaseName = 'invenanuanter'
    SQL.Strings = (
      
        'select codigo, almac, folio, serie, compro, concepto, fechasal, ' +
        'ptvta'
      'from movart a join conceps b on a.compro = b.ncon'
      
        'where a.codigo = :CODIGO and salio = '#39'S'#39' and salepor = 20 order ' +
        'by fechasal'
      ''
      ' ')
    Left = 368
    Top = 42
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'CODIGO'
        ParamType = ptUnknown
      end>
  end
