  object uqry_facturma: TUpdateSQL
    ModifySQL.Strings = (
      'update facturma'
      'set'
      '  MAYORIS = :MAYORIS,'
      '  REFER = :REFER,'
      '  NOMAY = :NOMAY,'
      '  DIR = :DIR,'
      '  RFC = :RFC,'
      '  STATUS = :STATUS,'
      '  FECHA = :FECHA,'
      '  VENCE = :VENCE,'
      '  IMPORTE = :IMPORTE,'
      '  DESCU = :DESCU,'
      '  NETO = :NETO,'
      '  IVA = :IVA,'
      '  TOTAL = :TOTAL,'
      '  CIA = :CIA,'
      '  NPAGOS = :NPAGOS,'
      '  PLAZO = :PLAZO,'
      '  TIPAGO = :TIPAGO,'
      '  PDSC = :PDSC,'
      '  MAYOMEN = :MAYOMEN,'
      '  IDUUID = :IDUUID'
      'where'
      '  IDFACTUR = :OLD_IDFACTUR')
    InsertSQL.Strings = (
      'insert into facturma'
      
        '  (SERIEFAC, NUM, MAYORIS, REFER, NOMAY, DIR, RFC, STATUS, FECHA' +
        ', '
      'VENCE, '
      
        '   IMPORTE, DESCU, NETO, IVA, TOTAL, CIA, NPAGOS, PLAZO, TIPAGO,' +
        ' PDSC, '
      '   MAYOMEN, IDFACTUR, TIPOFAC,   IDUUID'
      ')'
      'values'
      
        '  (:SERIEFAC, :NUM, :MAYORIS, :REFER, :NOMAY, :DIR, :RFC, :STATU' +
        'S, :FECHA, '
      
        '   :VENCE, :IMPORTE, :DESCU, :NETO, :IVA, :TOTAL, :CIA, :NPAGOS,' +
        ' :PLAZO, '
      '   :TIPAGO, :PDSC, :MAYOMEN, :IDFACTUR, :TIPOFAC,  :IDUUID )')
    DeleteSQL.Strings = (
      'delete from facturma'
      'where'
      '  IDFACTUR = :OLD_IDFACTUR')
    Left = 438
    Top = 375
  end
