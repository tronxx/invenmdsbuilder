  object uqry_promot: TUpdateSQL
    ModifySQL.Strings = (
      'update promotor'
      'set'
      '  TIENDA = :TIENDA,'
      '  CLAVE = :CLAVE,'
      '  NOMBRE = :NOMBRE,'
      '  UBI = :UBI,'
      '  NUMZON = :NUMZON,'
      '  CVEZON = :CVEZON,'
      '  POC = :POC,'
      '  COMLET = :COMLET,'
      '  COMREC = :COMREC,'
      '  STATUS = :STATUS'
      'where'
      '  CLAVE = :OLD_CLAVE')
    InsertSQL.Strings = (
      'insert into promotor'
      
        '  (TIENDA, CLAVE, NOMBRE, UBI, NUMZON, CVEZON, POC, COMLET, COMR' +
        'EC, '
      'STATUS)'
      'values'
      
        '  (:TIENDA, :CLAVE, :NOMBRE, :UBI, :NUMZON, :CVEZON, :POC, :COML' +
        'ET, '
      ':COMREC, '
      '   :STATUS)')
    DeleteSQL.Strings = (
      'delete from promotor'
      'where'
      '  CLAVE = :OLD_CLAVE')
    Left = 72
    Top = 200
  end
