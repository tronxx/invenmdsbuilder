  object qry_gpoinv: TQuery
    CachedUpdates = True
    AfterScroll = qry_gpoinvAfterScroll
    DatabaseName = 'manvehi'
    SQL.Strings = (
      
        'select  idgrupo, a.codigo, a.descri, impri, a.cia, a.status, a.f' +
        'ecbaj,'
      'a.idcatprodsat, b.codigo as codsat, b.descri as descrisat'
      
        'from inv_grupos a left outer join inv_catprodsat b on a.idcatpro' +
        'dsat = b.idprod'
      'where a.cia = :CIA  order by a.codigo'
      ' '
      ' ')
    UpdateObject = uqry_gpoinv
