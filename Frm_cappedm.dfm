  object qry_renentra: TQuery
    CachedUpdates = True
    DatabaseName = 'manvehi'
    SQL.Strings = (
      'select a.*, c.concepto, b.descri as descriinv, b.costos,'
      
        '( case codinv when '#39'AUXILIAR'#39' then concepto else descri end ) as' +
        ' descripcion'
      'from renentra  a'
      'left outer join conceps c on a.persenvrec = c.ncon'
      'left outer join inven b on a.codinv = b.codigo'
      'and a.cia = b.cia'
      'where  a.tipo= :Tipo and a.alm= :Alm and a.numero= :Numero'
      'and a.cia= :cia order by a.conse'
      ' '
