  object qry_entradas: TQuery
    CachedUpdates = True
    DatabaseName = 'manvehi'
    SQL.Strings = (
      'select'
      'tipo,alm,recemi,numero,facpro,prove,perenvrec,status,'
      'coniva,fecha,importe,iva, importe + iva as total,'
      
        'cast ( (case  vence when '#39'0000-00-00'#39' then fecha else vence end)' +
        ' as Date ) as vence,'
      'ctofin,tascomp,'
      'taspro,'
      
        'cast ( ( case  fechafac when '#39'0000-00-00'#39' then fecha else fechaf' +
        'ac end) as Date ) as fechafac,'
      'letras,plazocfp,planp,fletes,desxap,'
      
        'cast ( ( case  fechaprp when '#39'0000-00-00'#39' then fecha else fechap' +
        'rp end) as Date ) as fechaprp,'
      'ctofincomp,usuario,cia'
      'from Entradas'
      'where tipo= :Tipo and alm=:Alm and numero= :Numero and cia=:Cia'
      ''
      ' ')
    UpdateObject = uqsEntradas
    Left = 216
    Top = 368
    ParamData = <
      item
        DataType = ftString
        Name = 'tipo'
        ParamType = ptUnknown
      end
