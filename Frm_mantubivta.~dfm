  object qry_situac: TQuery
    CachedUpdates = True
    DatabaseName = 'manvehi'
    SQL.Strings = (
      
        'select (ubica || alm || pvta ) as llave, * from ubivtainv order ' +
        'by ubica, pvta, alm')
    UpdateObject = uqry_marcas
    Left = 352
    Top = 96
  end
  object uqry_marcas: TUpdateSQL
    ModifySQL.Strings = (
      'update ubivtainv'
      'set'
      '  ALM = :ALM,'
      '  PVTA = :PVTA,'
      '  ZONAINV = :ZONAINV,'
      '  VOC = :VOC,'
      '  ZONA2 = :ZONA2,'
      '  PTVTA2 = :PTVTA2,'
      '  ZONARTDES = :ZONARTDES,'
      '  ZONAPTVTA = :ZONAPTVTA,'
      '  ZONACAR = :ZONACAR,'
      '  SUBZONCAR = :SUBZONCAR,'
      '  UBICA = :UBICA'
      'where'
      '  ALM = :OLD_ALM and'
      '  PVTA = :OLD_PVTA and'
      '  UBICA = :OLD_UBICA')
    InsertSQL.Strings = (
      'insert into ubivtainv'
      
        '  (ALM, PVTA, ZONAINV, VOC, ZONA2, PTVTA2, ZONARTDES, ZONAPTVTA,' +
        ' '
      'ZONACAR, '
      '   SUBZONCAR, UBICA)'
      'values'
      
        '  (:ALM, :PVTA, :ZONAINV, :VOC, :ZONA2, :PTVTA2, :ZONARTDES, :ZO' +
        'NAPTVTA, '
      '   :ZONACAR, :SUBZONCAR, :UBICA)')
    DeleteSQL.Strings = (
      'delete from ubivtainv'
      'where'
      '  ALM = :OLD_ALM and'
      '  PVTA = :OLD_PVTA and'
      '  UBICA = :OLD_UBICA')
    Left = 382
    Top = 96
  end
