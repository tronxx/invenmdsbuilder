  object uqry_polizas: TUpdateSQL
    ModifySQL.Strings = (
      'update PolCob'
      'set'
      '  ALM = :ALM,'
      '  FECHA = :FECHA,'
      '  IMPORTE = :IMPORTE,'
      '  STATUS = :STATUS,'
      '  USUARIO = :USUARIO,'
      '  CIA = :CIA,'
      '  IDPOLCOB = :IDPOLCOB,'
      '  FOLIO = :FOLIO,'
      '  IDCONCEP = :IDCONCEP'
      'where'
      '  ALM = :OLD_ALM and'
      '  CIA = :OLD_CIA and'
      '  FOLIO = :OLD_FOLIO')
    InsertSQL.Strings = (
      'insert into PolCob'
      
        '  (ALM, FECHA, IMPORTE, STATUS, USUARIO, CIA, IDPOLCOB, FOLIO, I' +
        'DCONCEP)'
      'values'
      
        '  (:ALM, :FECHA, :IMPORTE, :STATUS, :USUARIO, :CIA, :IDPOLCOB, :' +
        'FOLIO, '
      '   :IDCONCEP)')
    DeleteSQL.Strings = (
      'delete from PolCob'
      'where'
      '  ALM = :OLD_ALM and'
      '  CIA = :OLD_CIA and'
      '  FOLIO = :OLD_FOLIO')
    Left = 56
    Top = 344
  end
  object uqry_renpolco: TUpdateSQL
    ModifySQL.Strings = (
      'update renpolco'
      'set'
      '  ALM = :ALM,'
      '  FECHA = :FECHA,'
      '  NUMREN = :NUMREN,'
      '  DOCTO = :DOCTO,'
      '  REFER = :REFER,'
      '  CONCEP = :CONCEP,'
      '  IMPORTE = :IMPORTE,'
      '  CLIENTE = :CLIENTE,'
      '  LETRA = :LETRA,'
      '  CIA = :CIA,'
      '  TIPAGO = :TIPAGO,'
      '  IDPOLCOB = :IDPOLCOB,'
      '  FOLIO = :FOLIO,'
      '  IDRENPOLCO = :IDRENPOLCO,'
      '  IDCONCEP = :IDCONCEP,'
      '  TIPOFAC = :TIPOFAC'
      'where'
      '  IDRENPOLCO = :OLD_IDRENPOLCO')
    InsertSQL.Strings = (
      'insert into renpolco'
      '  (ALM, FECHA, NUMREN, DOCTO, REFER, CONCEP, IMPORTE, CLIENTE, '
      'LETRA, CIA, '
      '   TIPAGO, IDPOLCOB, FOLIO, IDRENPOLCO, IDCONCEP, TIPOFAC)'
      'values'
      
        '  (:ALM, :FECHA, :NUMREN, :DOCTO, :REFER, :CONCEP, :IMPORTE, :CL' +
        'IENTE, '
      
        '   :LETRA, :CIA, :TIPAGO, :IDPOLCOB, :FOLIO, :IDRENPOLCO, :IDCON' +
        'CEP, '
      ':TIPOFAC)')
    DeleteSQL.Strings = (
      'delete from renpolco'
      'where'
      '  IDRENPOLCO = :OLD_IDRENPOLCO')
    Left = 48
    Top = 232
  end
