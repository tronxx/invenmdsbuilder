  object qry_pedimen: TQuery
    DatabaseName = 'manvehi'
    SQL.Strings = (
      
        'select b.concep as seriemot, c.concep as pedto, d.concep as adua' +
        'na, fechaped'
      'from  inv_invhist e '
      'join inv_datosent a on e.idart = a.idart'
      'join inv_conceps b on a.idseriemot = b.idconcep'
      'join inv_conceps c on a.idpedto  = c.idconcep'
      'join inv_conceps d on a.idaduana  = d.idconcep'
      'where e.codigo = :CODIGO and a.serie = :SERIE')
    Left = 472
    Top = 328
    ParamData = <
      item
        DataType = ftString
        Name = 'CODIGO'
        ParamType = ptUnknown
      end
      item
        DataType = ftString
        Name = 'SERIE'
        ParamType = ptUnknown
      end>
  end
