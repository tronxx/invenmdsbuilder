  object qry_movart: TQuery
    OnCalcFields = qry_movartCalcFields
    DatabaseName = 'manvehi'
    Filtered = True
    SQL.Strings = (
      
        'select codigo, almac, folio, prove, nompro, compro, facpro, fech' +
        'a, costo, sespe,'
      
        'modsal, modent, nentrada, tipo, vienede, folviene, vahacia, folr' +
        'ec, pueblo,'
      'numfac, seriefac, salio, smay, fechasal,'
      'canti, serie, salvta, entcan,'
      
        'nsalida, entrapor, salepor, fecentori, fecvencto, usuario, cia, ' +
        'ptvta,'
      'vend from movart '
      
        'where codigo = :CODIGO and almac = :ALM and folio = :FOLIO and c' +
        'ia = :CIA'
      ' ')
    Left = 441
    Top = 264
    ParamData = <
      item
        DataType = ftString
        Name = 'CODIGO'
        ParamType = ptUnknown
      end
      item
        DataType = ftString
        Name = 'ALM'
        ParamType = ptUnknown
      end
      item
        DataType = ftInteger
        Name = 'FOLIO'
        ParamType = ptUnknown
      end
      item
        DataType = ftInteger
        Name = 'CIA'
        ParamType = ptUnknown
      end>
