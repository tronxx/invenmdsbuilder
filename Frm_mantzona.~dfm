  object qry_zonas: TQuery
    CachedUpdates = True
    DatabaseName = 'manvehi'
    SQL.Strings = (
      'select * from zonas '
      'order by zona')
    UpdateObject = uqry_ptovta
    Left = 312
    Top = 32
  end
  object uqry_ptovta: TUpdateSQL
    ModifySQL.Strings = (
      'update zonas'
      'set'
      '  NOMBRE = :NOMBRE'
      'where'
      '  ZONA = :OLD_ZONA')
    InsertSQL.Strings = (
      'insert into zonas'
      '  (ZONA, NOMBRE)'
      'values'
      '  (:ZONA, :NOMBRE)')
    DeleteSQL.Strings = (
      'delete from zonas'
      'where'
      '  ZONA = :OLD_ZONA and')
    Left = 342
    Top = 32
  end
