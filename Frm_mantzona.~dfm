  object qry_megazonasdet: TQuery
    CachedUpdates = True
    DatabaseName = 'manvehi'
    SQL.Strings = (
      'select'
      'a.idmegazonadet,'
      'a.idmegazona,'
      'a.zona,'
      'a.cia,'
      'b.nombre'
      
        'from inv_megazonasdet a join zonas b on a.zona = b.zona and a.ci' +
        'a = b.cia'
      
        'where a.idmegazona = :IDMEGAZONA and a.cia = :CIA order by a.zon' +
        'a'
      '')
    UpdateObject = uqry_megazonasdet
    Left = 472
    Top = 32
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'IDMEGAZONA'
        ParamType = ptUnknown
      end
      item
        DataType = ftInteger
        Name = 'CIA'
        ParamType = ptUnknown
      end>
  end
