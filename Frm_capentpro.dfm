      '  DESXAP = :DESXAP,'
      '  FECHAPRP = :FECHAPRP,'
      '  CTOFINCOMP = :CTOFINCOMP,'
      '  USUARIO = :USUARIO'
      'where'
      '  TIPO = :OLD_TIPO and'
