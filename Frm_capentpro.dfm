      '  TASCOMP = :TASCOMP,'
      '  TASPRO = :TASPRO,'
      '  FECHAFAC = :FECHAFAC,'
      '  LETRAS = :LETRAS,'
      '  PLAZOCFP = :PLAZOCFP,'
      '  PLANP = :PLANP,'
