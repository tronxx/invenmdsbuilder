  object qry_marcas: TQuery
    CachedUpdates = True
    AfterScroll = qry_marcasAfterScroll
    DatabaseName = 'manvehi'
    SQL.Strings = (
      'select idmarcainv, codigo, marca from inv_marcas'
      'order by codigo')
    UpdateObject = uqry_marcas
    Left = 320
    Top = 85
  end
  object uqry_marcas: TUpdateSQL
    ModifySQL.Strings = (
      'update inv_marcas'
      'set'
      '  MARCA = :MARCA'
      'where'
      '  IDMARCAINV = :OLD_IDMARCAINV')
    InsertSQL.Strings = (
      'insert into inv_marcas'
      '  (IDMARCAINV, CODIGO, MARCA)'
      'values'
      '  (:IDMARCAINV, :CODIGO, :MARCA)')
    DeleteSQL.Strings = (
      'delete from inv_marcas'
      'where'
      '  IDMARCAINV = :OLD_IDMARCAINV')
    Left = 350
    Top = 85
  end
