  object qry_usuario: TQuery
    CachedUpdates = True
    DatabaseName = 'manvehi'
    SQL.Strings = (
      
        'select a.idusuario, a.idnombre, a.login, a.clave, a.maestro, a.n' +
        'umpol,'
      'a.iniciales, a.status, a.fecbaj, b.nombre'
      'from car_usuarios  a join pvenfimo b on a.idnombre = b.idnombre'
      'order by login'
      ' ')
    UpdateObject = uqry_usuario
    Left = 120
    Top = 232
  end
  object uqry_usuario: TUpdateSQL
    ModifySQL.Strings = (
      'update car_usuarios'
      'set'
      '  IDUSUARIO = :IDUSUARIO,'
      '  IDNOMBRE = :IDNOMBRE,'
      '  LOGIN = :LOGIN,'
      '  CLAVE = :CLAVE,'
      '  MAESTRO = :MAESTRO,'
      '  NUMPOL = :NUMPOL,'
      '  INICIALES = :INICIALES'
      'where'
      '  IDUSUARIO = :OLD_IDUSUARIO')
    InsertSQL.Strings = (
      'insert into car_usuarios'
      
        '  (IDUSUARIO, IDNOMBRE, LOGIN, CLAVE, MAESTRO, NUMPOL, INICIALES' +
        ')'
      'values'
      
        '  (:IDUSUARIO, :IDNOMBRE, :LOGIN, :CLAVE, :MAESTRO, :NUMPOL, :IN' +
        'ICIALES)')
    DeleteSQL.Strings = (
      'delete from car_usuarios'
      'where'
      '  IDUSUARIO = :OLD_IDUSUARIO')
    Left = 150
    Top = 232
  end
